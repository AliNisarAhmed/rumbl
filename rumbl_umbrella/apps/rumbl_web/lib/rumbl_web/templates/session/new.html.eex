<h1>Login</h1>

<%= form_for @conn,
						 Routes.session_path(@conn, :create),
						 [as: :session],
						 fn formData -> %>
	<div>
		<%= text_input formData, :username, placeholder: "Username" %>
	</div>
	<div>
		<%= password_input formData, :password, placeholder: "Password" %>
	</div>
	<%= submit "Log in" %>
<% end %>
